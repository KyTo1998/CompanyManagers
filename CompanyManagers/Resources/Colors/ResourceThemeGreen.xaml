    <Brush x:Key="colorTextForTheme">#3BA59B</Brush>
    <Brush x:Key="colorMoveMouseItem">#196CD495</Brush>
    <Brush x:Key="colorTextMessage">#FFFFFF</Brush>
    <Brush x:Key="colorTextMessageLink">#FF0562FF</Brush>
    <Brush x:Key="colorSlection">#4C6CD495</Brush>
    <Brush x:Key="#D0D0D0">#D0D0D0</Brush>
</ResourceDictionary>