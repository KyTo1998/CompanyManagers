<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="colorTheme" EndPoint="1,1" StartPoint="0,1">
        <GradientStop Color="#7D79D9" Offset="0"/>
        <GradientStop Color="#AA82C1" Offset="0.5"/>
        <GradientStop Color="#DD8CA2" Offset="1"/>
    </LinearGradientBrush>
    <Brush x:Key="colorTextForTheme">#7D79D9</Brush>
    <Brush x:Key="colorMoveMouseItem">#19DD8CA2</Brush>
    <Brush x:Key="colorTextMessage">#FFFFFF</Brush>
    <Brush x:Key="colorTextMessageLink">#FFFFFF</Brush>
    <Brush x:Key="colorSlection">#4CDD8CA2</Brush>
    <Brush x:Key="#D0D0D0">#D0D0D0</Brush>
</ResourceDictionary>