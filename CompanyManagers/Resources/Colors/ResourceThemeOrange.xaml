<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <LinearGradientBrush x:Key="colorTheme" EndPoint="1,1" StartPoint="0,1">
        <GradientStop Color="#FC9C80" Offset="0"/>
        <GradientStop Color="#FC9C80" Offset="0.5"/>
        <GradientStop Color="#FCC88C" Offset="1"/>
    </LinearGradientBrush>
    <Brush x:Key="colorTextForTheme">#FC9C80</Brush>
    <Brush x:Key="colorMoveMouseItem">#19FCC88C</Brush>
    <Brush x:Key="colorTextMessage">#FFFFFF</Brush>
    <Brush x:Key="colorTextMessageLink">#FF0562FF</Brush>
    <Brush x:Key="colorSlection">#4CFCC88C</Brush>
    <Brush x:Key="#D0D0D0">#D0D0D0</Brush>
</ResourceDictionary>